{
  "name": "linkaTeca",
  "version": "0.1.0",
  "description": "extension to organize links to articles, books, posts",
  "icons": {
    "16": "src/assets/picon16.png",
    "32": "src/assets/picon32.png",
    "48": "src/assets/picon48.png",
    "128": "src/assets/picon128.png"
  },
  "permissions": [
    "storage",
    "activeTab",
    "downloads",
    "unlimitedStorage"
  ],
  "web_accessible_resources": [
    {
      "matches": [
        "<all_urls>"
      ],
      "resources": [
        "_favicon/*"
      ],
      "use_dynamic_url": false
    },
    {
      "matches": [
        "<all_urls>"
      ],
      "resources": [
        "assets/content.ts-740db5e0.js"
      ],
      "use_dynamic_url": true
    }
  ],
  "background": {
    "service_worker": "service-worker-loader.js",
    "type": "module"
  },
  "content_scripts": [
    {
      "js": [
        "assets/content.ts-loader-96fee152.js"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "action": {
    "default_popup": "index.html"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "manifest_version": 3
}
