{
  "name": "linkaTeca",
  "version": "0.2.0",
  "description": "extension to organize links to articles, books, posts",
  "icons": {
    "16": "src/assets/picon16.png",
    "32": "src/assets/picon32.png",
    "48": "src/assets/picon48.png",
    "128": "src/assets/picon128.png"
  },
  "permissions": [
    "storage",
    "activeTab",
    "downloads",
    "unlimitedStorage",
    "contextMenus"
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "_favicon/*"
      ],
      "extension_ids": [
        "*"
      ]
    },
    {
      "use_dynamic_url": true,
      "matches": [
        "<all_urls>"
      ],
      "resources": [
        "**/*",
        "*"
      ]
    }
  ],
  "background": {
    "service_worker": "service-worker-loader.js",
    "type": "module"
  },
  "content_scripts": [
    {
      "matches": [
        "https://chat.openai.com/*"
      ],
      "js": [
        "src/content.ts-loader.js"
      ]
    }
  ],
  "action": {
    "default_popup": "index.html"
  },
  "manifest_version": 3
}
